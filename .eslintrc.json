{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "curly": "warn",
        "no-restricted-syntax": [
            "off",
            {
                "selector": "ForOfStatement"
            }
        ],
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off"
    }
}
